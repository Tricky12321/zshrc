[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
  new_tab = <Primary>t
  cycle_next = <Shift><Alt>Left
  cycle_prev = <Shift><Alt>Right
  close_term = <Primary>w
  next_tab = <Primary>Tab
  prev_tab = <Primary><Shift>Tab
  new_window = <Primary>n
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    scrollback_lines = 50000
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
